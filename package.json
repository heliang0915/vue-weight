{
  "name": "vue-weight",
  "version": "1.0.0",
  "description": "vue-weight",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=production&&node server/app.js",
    "build": "rimraf dist && npm run build:client && npm run build:server&&npm run start",
    "build:client": "export NODE_ENV=production&&webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "export NODE_ENV=production&&webpack --config build/webpack.server.config.js --progress --hide-modules",
    "dev_server": "cross-env NODE_ENV=development&& supervisor -w server,src,assets server/app.js",
    "dev": "rimraf dist && npm run dev:client&&npm run dev_server",
    "dev:client": "cross-env NODE_ENV=development&& webpack --config build/webpack.client.config.js --watch --progress --hide-modules",
    "dev:server": "cross-env NODE_ENV=development&& webpack --config build/webpack.server.config.js --watch --progress --hide-modules"
  },
  "author": "heliang0915",
  "license": "ISC",
  "dependencies": {
    "asset-require-hook": "^1.1.0",
    "autoprefixer": "^7.1.2",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-helpers": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "convert-source-map": "^1.5.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "css-modules-require-hook": "^4.0.6",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.29.0",
    "isomorphic-fetch": "^2.2.1",
    "log4js": "^0.6.15",
    "morgan": "^1.8.2",
    "open-browser-webpack-plugin": "0.0.5",
    "pseudomap": "^1.0.2",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.9",
    "vue": "^2.3.2",
    "vue-loader": "^13.0.2",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.3.2",
    "vue-template-compiler": "^2.3.2",
    "vuex": "^2.3.1",
    "webpack": "^3.1.0",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0",
    "yallist": "^3.0.2"
  }
}
